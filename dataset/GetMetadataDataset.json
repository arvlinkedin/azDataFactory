{
	"name": "GetMetadataDataset",
	"properties": {
		"description": "Connection to your source data store.",
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SourceDirectory": {
				"type": "string",
				"defaultValue": "movies"
			},
			"SourceContainer": {
				"type": "string",
				"defaultValue": "adftest1"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "moviesDB2.csv",
				"folderPath": {
					"value": "@dataset().SourceDirectory",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().SourceContainer",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}